<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app 
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" 
    "http://java.sun.com/dtd/web-app_2_3.dtd">

 <web-app>
   <display-name>Indexer Client Servlet</display-name>
   <servlet>
     <servlet-name>IndexerClientServlet</servlet-name>
     <servlet-class>indexer.servlets.IndexerClientServlet</servlet-class> 
   </servlet>
    
   <servlet-mapping>
     <servlet-name>IndexerClientServlet</servlet-name>
     <url-pattern>/indexerclient</url-pattern>
   </servlet-mapping> 
    
</web-app>

