    </servlet>
    <servlet-mapping>
        <servlet-name>TestResponse</servlet-name>
        <url-pattern>/testResponse</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>TestCookie</servlet-name>
        <servlet-class>edu.upenn.cis.cis455.TestCookieServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TestCookie</servlet-name>
        <url-pattern>/testCookie</url-pattern>
    </servlet-mapping>
    <servlet>
