    </servlet>
    <servlet-mapping>
        <servlet-name>ChannelManagementServlet</servlet-name>
        <url-pattern>/channels</url-pattern>
    </servlet-mapping>
     <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>edu.upenn.cis455.servlet.LogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DeleteChannelServlet</servlet-name>
        <servlet-class>edu.upenn.cis455.servlet.DeleteChannelServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteChannelServlet</servlet-name>
        <url-pattern>/channel/delete</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>URLServlet</servlet-name>
        <servlet-class>edu.upenn.cis455.servlet.URLServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>URLServlet</servlet-name>
        <url-pattern>/urls</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>MasterServlet</servlet-name>
        <servlet-class>edu.upenn.cis455.mapreduce.master.MasterServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>MasterServlet</servlet-name>
        <url-pattern>/master/*</url-pattern>
    </servlet-mapping>

    
	<servlet>
        <servlet-name>WorkerServlet</servlet-name>
        <servlet-class>edu.upenn.cis455.mapreduce.worker.WorkerServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>WorkerServlet</servlet-name>
        <url-pattern>/worker/*</url-pattern>
    </servlet-mapping>

     <context-param>
     	<param-name>BDBStore</param-name>
     	<param-value>resources/BDBStore</param-value>
     </context-param>
     
     <context-param>
     	<param-name>master</param-name>
     	<param-value>127.0.0.1:8080</param-value>
     </context-param>
     
      <context-param>
     	<param-name>storagedir</param-name>
     	<param-value>resources/worker1</param-value>
     </context-param>
     
     <context-param>
     	<param-name>worker-port</param-name>
     	<param-value>8080</param-value>
     </context-param>
     
     <context-param>
     	<param-name>worker-ip</param-name>
     	<param-value>localhost</param-value>
     </context-param>
</web-app>
