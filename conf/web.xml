<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE web-app
     PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
     "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
 
 <web-app>
     <display-name>Test servlet for CIS455</display-name>

    <servlet>
        <servlet-name>SearchEngineInterface</servlet-name>
        <servlet-class>searchengine.servlets.SearchEngineInterface</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>SearchEngineInterface</servlet-name>
        <url-pattern>/searchInterface/*</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>IndexerClientServlet</servlet-name>
        <servlet-class>searchengine.servlets.IndexerClientServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>IndexerClientServlet</servlet-name>
        <url-pattern>/search/*</url-pattern>
    </servlet-mapping>
    

</web-app>
