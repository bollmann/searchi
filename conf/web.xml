        <servlet-class>edu.upenn.cis.cis455.TestSessionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TestSession</servlet-name>
        <url-pattern>/testSession</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>TestErrors</servlet-name>
        <servlet-class>edu.upenn.cis.cis455.TestErrorsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TestErrors</servlet-name>
        <url-pattern>/testErrors</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>TestParameters</servlet-name>
        <servlet-class>edu.upenn.cis.cis455.TestParametersServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TestParameters</servlet-name>
        <url-pattern>/testParameters</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>BusyServlet</servlet-name>
        <servlet-class>edu.upenn.cis.cis455.BusyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BusyServlet</servlet-name>
        <url-pattern>/busyServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>XPathServlet</servlet-name>
        <servlet-class>edu.upenn.cis455.servlet.XPathServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>XPathServlet</servlet-name>
        <url-pattern>/xPathServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UserLoginServlet</servlet-name>
        <servlet-class>edu.upenn.cis455.servlet.UserLoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserLoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UserManagementServlet</servlet-name>
        <servlet-class>edu.upenn.cis455.servlet.UserManagementServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserManagementServlet</servlet-name>
        <url-pattern>/user</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>NewUserServlet</servlet-name>
        <servlet-class>edu.upenn.cis455.servlet.NewUserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewUserServlet</servlet-name>
        <url-pattern>/user/create</url-pattern>
    </servlet-mapping>
     <servlet>
