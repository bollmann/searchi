h3 Image results for '#{q}'
hr
br
.blueimp-gallery.blueimp-gallery-controls#blueimp-gallery(data-use-bootstrap-modal='false')
  // The container for the modal slides
  .slides
  // Controls for the borderless lightbox
  h3.title Title
  a.prev ‹
  a.next ›
  a.close ×
  a.play-pause
  ol.indicator
  // The modal dialog, which will be used to wrap the lightbox content
  .modal.fade
    .modal-dialog
      .modal-content
        .modal-header
          button.close(type='button', aria-hidden='true') ×
          h4.modal-title
        .modal-body.next
        .modal-footer
          button.btn.btn-default.pull-left.prev(type='button')
            i.glyphicon.glyphicon-chevron-left
            |                         Previous
          button.btn.btn-primary.next(type='button')
            | Next
            i.glyphicon.glyphicon-chevron-right

.links
  for url in urls
    a(href='#{url.url}', data-gallery='')
      img(src='#{url.url}', width='200')
