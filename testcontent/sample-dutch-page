<script>jQuery.noConflict();</script>
<script>
		jQuery(function () {
		  var snippets = jQuery('[ikea-snippet]');
		  // Snippet whitelist, just put commas between the selectors
          snippets = snippets.filter('[ikea-snippet*="article-overview"], [ikea-snippet*="campaign-"], [ikea-snippet*="cta-"], [ikea-snippet*="help"], [ikea-snippet*="homepage-"], [ikea-snippet*="product-closed"], [ikea-snippet*="product-viewer"], [ikea-snippet="related-large"], [ikea-snippet="related"], [ikea-snippet*="quote-"]');
          for (var i = 0; i < snippets.length; i++) {
            var $thisSnippet = jQuery(snippets[i]);
            var links = $thisSnippet.find('a').not('[data-role="link-overlay"]'); // Filter out cases where link-overlay must be added manually because there is no other link in the block for the script to use
            if (links.length == 1) {
              var $thisLink = links,
                  url = $thisLink.attr('href'),
                  text = $thisLink.text();
              var $newLink = jQuery(document.createElement('a')),
                  $newText = jQuery(document.createElement('span'));
              $newText.text(text).appendTo($newLink);
              $newLink.attr({'href': url, 'data-role': 'link-overlay', 'class': 'link-overlay'}).appendTo($thisSnippet);
            }
          }
          jQuery('[data-role="link-overlay"]').hover(
            function() {
              jQuery(this).closest('[ikea-snippet]').find('a, .button, .ken-burns').not('.no-link-all-hover').addClass('hover');
            },
            function() {
              jQuery(this).closest('[ikea-snippet]').find('a, .button, .ken-burns').removeClass('hover');
            }
          );
		});
	</script>
<script>
    jQuery(function () {
        jQuery(document).keydown(function (event) {
            // Listen for the tab and arrow keys, otherwise, end here
            if ([9, 37, 38, 39, 40].indexOf(event.keyCode) == -1) {
              return;
            }
            jQuery('html').addClass('keyboard-navigation');
        });
    });
  </script>
<script>
      function checkForIE () {
        var trident = navigator.userAgent.match(/Trident\/(\d+)/);
        if (trident) {
          var msie = navigator.userAgent.match(/MSIE (\d+)/);
          if (msie) {
            // If compatibility mode
            if (parseInt(msie[1]) != (parseInt(trident[1]) + 4)) {
              document.documentElement.setAttribute('class', 'ie-compat');
            }
            // If less than ie 10
            if (parseInt(msie[1]) < 10) {
              document.documentElement.className += ' ie-lt-10';
            }
            // If less than ie 9
            if (parseInt(msie[1]) < 9) {
              document.documentElement.className += ' ie-lt-9';
            }
            // If compatibility mode and less than ie 9
            if (parseInt(msie[1]) < 9 && parseInt(msie[1]) != (parseInt(trident[1]) + 4)) {
              // alert('It looks like this site is shown in Compatibitility View. This could cause some display problems. '+
              // '\n\nUnder Tools (gear icon) -> Compatibility View settings, turn this mode off for IKEA.nl or intranet sites');
              alert('Het lijkt erop dat deze site in Compatibiliteitsweergave wordt getoond. Dit zou weergaveproblemen kunnen veroorzaken. ' +
              '\n\nSchakel deze modus uit bij Extra (tandwielicoon) -> Instellingen voor Compatibiliteitsweergave, voor IKEA.nl of intranet sites');
            }
          }
        }

      };
      checkForIE();
	</script></div>
