<script>
	  jQuery(function () {
		  var sliders = document.querySelectorAll('[data-role="slider"]');
          for (var i = 0; i < sliders.length; i++) {
            var $slider = jQuery(sliders[i]),
              slidesPerView = $slider.data('slidesPerView'),
              autoPlay = $slider.data('autoplay'),
	            $sliderPrev = $slider.find('[data-role="slider-prev"]'),
		        $sliderNext = $slider.find('[data-role="slider-next"]');
            var $thisSlider = new Swiper ($slider, {
              nextButton: $sliderNext,
              prevButton: $sliderPrev,
              loop: true,
              autoplay: autoPlay,
              slidesPerView: slidesPerView ? slidesPerView : 1,
              slidesPerGroup: slidesPerView ? slidesPerView : 1
            });
          };
	  });
