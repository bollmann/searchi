<script>
  jQuery(function() {

    var $snippet = jQuery('[data-role="homepage-slideshow"]'),
        $overlay = jQuery('[data-role="homepage-slideshow-overlay"]'),
        $close = jQuery('[data-role="homepage-start-shopping"]');

    if (!(/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent))) {
      $snippet.remove();
    } else {
      if (!$snippet.hasClass('header')) {
        jQuery('#allContent').wrap(function () {
          return '<div id="allContentWrapper"></div>';
        });
        jQuery('body').prepend($snippet);

        jQuery('html, body').on('scroll mousedown wheel DOMMouseScroll mousewheel keyup touchmove', function (e) {
          jQuery('html, body').stop();
        });

        jQuery($close).on('click', function () {
          jQuery('html, body').animate({
            scrollTop: $snippet.height()
          }, 1500, 'easeInOutQuint');
        });
      }


      function resizeIt() {
        var $slideshowSnippet = jQuery('[data-role="homepage-slideshow"]');
        $slideshowSnippet.css('height', ($slideshowSnippet.width() / jQuery(window).width()) * jQuery(window).height());
      }
      resizeIt();
      jQuery(window).resize(function () {
        resizeIt();
      });


      if (!$snippet.hasClass('header')) {
        if (jQuery.cookie('showHomepagevideo') != undefined) {
          jQuery('html, body').scrollTop($snippet.height());
        } else {
          jQuery.cookie('showHomepagevideo', 'youtube');
        }
      }
    }
  });
  </script>
  <script>
    jQuery(function () {

      var $snippet = jQuery('[ikea-snippet="homepage-video"]'),
        $wrapper = jQuery('[data-role="homepage-video-wrapper"]'),
        $curtain = jQuery('[data-role="homepage-video-curtain"]'),
        $overlay = jQuery('[data-role="homepage-video-overlay"]'),
        $logo = jQuery('[data-role="homepage-logo"]'),
        $close = jQuery('[data-role="homepage-start-shopping"]');

      if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {

        $snippet.remove();

      } else {

         if (!$snippet.hasClass('header')) {

           jQuery('#allContent').wrap(function () {
             return '<div id="allContentWrapper"></div>';
           });

           jQuery('body').prepend($snippet);

           jQuery('html, body').on('scroll mousedown wheel DOMMouseScroll mousewheel keyup touchmove', function (e) {
             jQuery('html, body').stop();
           });

           jQuery($close).on('click', function () {
             jQuery('html, body').animate({
               scrollTop: $snippet.height()
             }, 1500, 'easeInOutQuint');
           });

          function resizeIt() {
            var windowWidth = jQuery(window).width(),
              windowHeight = jQuery(window).height(),
              videoWidth = windowWidth,
              videoHeight = (videoWidth / 16) * 9;

            if (videoHeight < windowHeight) {
              videoHeight = windowHeight;
              videoWidth = (videoHeight / 9) * 16;
            }


            $snippet.css('width', jQuery(window).width()); // Step 1
            $snippet.css('width', $curtain.width()); // Step 2
            $snippet.css('height', ($snippet.width() / jQuery(window).width()) * jQuery(window).height());
            $wrapper.css({'width': videoWidth, 'height': videoHeight});
          }
          resizeIt();
          jQuery(window).resize(function () {
            resizeIt();
          });
         }

        function bindEvent(el, eventName, eventHandler) {
          if (el.addEventListener){
            el.addEventListener(eventName, eventHandler, false);
          } else if (el.attachEvent){
            el.attachEvent('on'+eventName, eventHandler);
          }
        }

        if (!$snippet.hasClass('header')) {
          bindEvent(document, "scroll", function (e) {
            var minY = 0;
            var maxY = 700;

            if ($logo.css('position') == 'absolute')
              $logo.css('position', 'fixed');

            if (jQuery(window).scrollTop() >= minY && jQuery(window).scrollTop() < maxY) {
              var ratio = jQuery(window).scrollTop() / maxY;
              $overlay.css('opacity', ratio * 0.7);
            }
          });

          if (jQuery.cookie('showHomepagevideo') != undefined) {
            jQuery('html, body').scrollTop($snippet.height());
          } else {
            jQuery.cookie('showHomepagevideo', 'youtube');
          }
        }
      }
    });

    function onYouTubePlayerAPIReady() {
      var players = document.querySelectorAll('[data-role="homepage-youtube-video"]');
      var youtubeHomepageWidth = jQuery(window).width();
      var youtubeHomepageHeight = (youtubeHomepageWidth / 16) * 9;
      for (var i = 0; i < players.length; i++) {
        new YT.Player(players[i], {
          width: '100%',
          height: '100%',
          videoId: jQuery(players[i]).attr('data-id'),
          playerVars: {
            'autohide': 1,
            'loop': 1,
            'controls': 0,
            'showinfo': 0,
            'modestbranding': 1,
            'wmode': 'opaque',
            'iv_load_policy': 3
          },
          events: {
            'onReady': onPlayerReady,
            'onStateChange': onPlayerStateChange
          }
        });
      }
    }
    function onPlayerReady(event) {
      event.target.setVolume(0);
      event.target.playVideo();
    }
    function onPlayerStateChange(event) {

      if (event.data === YT.PlayerState.PLAYING) {
        jQuery('[data-role="homepage-video-curtain"]').addClass('faded-out');
      }
      if (event.data === YT.PlayerState.ENDED) {
        event.target.playVideo();
      }
    }
  </script></div>
