<script>
    
    function moveLeftTelenor(){
        
        var $w = $('.telenor-widget');
        var $a = $w.find('.telenor-widget-group.active');
        if($a.length){
            $a.removeClass('active');
            var $next = $a.next('.telenor-widget-group');
            if($next.length){
                $next.addClass('active');
            } else {
                $('.telenor-widget-group:nth-child(1)').addClass('active');
            }
        }
    }
    
    function moveRightTelenor(){
        var $w = $('.telenor-widget');
        var $a = $w.find('.telenor-widget-group.active');
        if($a.length){
            $a.removeClass('active');
            var $next = $a.prev('.telenor-widget-group');
            if($next.length){
                $next.addClass('active');
            } else {
                $('.telenor-widget-group:last-child').addClass('active');
            }
        }
    }
    
</script>        
                <div class="article-list-item top-right-border">
        <span class="category InfoPage">Info</span>
