                        <h2 class="o-heading">GET CAREER ADVICE FROM OUR EXPERTS</h2>
                        <!--------------------------------------------------------------------------------------------->
                        <!-- TABS SECTION -->
                        <section class="c-tabs">
                            <div role="tabpanel">

                                <!-- Nav tabs -->
                                <ul class="nav nav-tabs" role="tablist">
                                    <li role="presentation" class="active"><a href="#resume-builder"
                                                                              aria-controls="resume-builder"
                                                                              role="tab" data-toggle="tab">Resume
                                        Builder</a></li>
