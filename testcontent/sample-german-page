    try
    {
        var ystr="";
        var y_adj="";

        for (var id in yl.YpResult.getAll()) {

            c = yl.YpResult.get(id);
            ystr+= ';y_ad='+c.id;
            if(c.format){
                y_adj=';y_adj='+c.format;
            }
        }
        ystr+=y_adj+';';
        WLRCMD=WLRCMD+ystr+segQS+crtg_content;
